<?xml version="1.0" encoding="utf-8"?>
<!-- -->
<RelativeLayout android:id="@+id/nav_title_menu"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<!-- -->
	<include android:id="@+id/title_sub_panel" layout="@layout/title_bar">
	</include>
	<!-- -->
	<FrameLayout android:id="@+id/ad_panel_"
			android:layout_alignParentBottom="true" android:layout_width="fill_parent"
			android:layout_height="wrap_content">
			<include layout="@layout/ad_panel" />
		</FrameLayout>
	<!-- -->
	<WebView android:id="@+id/webview"
		android:layout_below="@id/title_sub_panel" android:layout_above="@id/ad_panel_"
		android:layout_width="fill_parent" android:layout_height="fill_parent" />
	<!-- -->
	<LinearLayout android:orientation="vertical"
		android:layout_centerInParent="true" android:layout_width="wrap_content"
		android:layout_height="wrap_content">
		<ProgressBar android:id="@+id/progressImage"
		style="@android:style/Widget.ProgressBar.Inverse" mce_style="@android:style/Widget.ProgressBar.Inverse"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:visibility="gone" />
		<TextView android:id="@+id/progressText" android:textStyle="bold"
			android:textSize="16dip" android:textColor="#FFFFFFFF"
			android:layout_marginTop="10dip" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
		</TextView>
	</LinearLayout>
</RelativeLayout>
