<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res/mobi.domore.mcdonalds"
	android:id="@+id/ad_panel" android:gravity="center"
	android:orientation="vertical" android:background="#000000">
	<!--   -->
	<com.wooboo.adlib_android.WoobooAdView android:id="@+id/adview"
		android:layout_width="fill_parent" android:layout_height="48dip"
		app:testing="false" app:backgroundColor="#00000000"
		app:isGoneWithoutAd="true" app:textColor="#FFFF0000"
		app:refreshInterval="20" />
</LinearLayout>